{
    "ci": {
        "collect": {
            "startServerCommand": "npm run start",
            "url": [
                "http://localhost:3000/",
                "http://localhost:3000/login"
            ],
            "numberOfRuns": 3
        },
        "assert": {
            "preset": "lighthouse:no-pwa",
            "assertions": {
                "categories:performance": [
                    "warn",
                    {
                        "minScore": 0.9
                    }
                ],
                "categories:accessibility": [
                    "error",
                    {
                        "minScore": 0.95
                    }
                ],
                "categories:best-practices": [
                    "error",
                    {
                        "minScore": 0.95
                    }
                ],
                "categories:seo": [
                    "error",
                    {
                        "minScore": 1
                    }
                ]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}